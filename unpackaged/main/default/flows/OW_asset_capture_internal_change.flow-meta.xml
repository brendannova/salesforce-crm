    <decisions>
        <name>Display_in_profile</name>
        <label>Display in profile?</label>
        <locationX>446</locationX>
        <locationY>455</locationY>
        <defaultConnector>
            <targetReference>Clear_hideFromProfile_no_reason</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Yes - no reason to hide</defaultConnectorLabel>
        <rules>
            <name>Yes_due_to_status</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.OW_status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Owned</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Clear_hideFromProfile</targetReference>
            </connector>
            <label>Yes - due to status</label>
        </rules>
        <rules>
            <name>No_hide_due_to_status</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.OW_status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Duplicate</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OW_status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sold</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OW_status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Surrendered</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_hideFromProfile</targetReference>
            </connector>
            <label>No - hide due to status</label>
        </rules>
        <rules>
            <name>No_deleted_by_client</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.OW_status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Deleted by client</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_hideFromProfile_deleted_by_client</targetReference>
            </connector>
            <label>No - deleted by client</label>
        </rules>
    </decisions>
    <description>This flow will run whenever criteria defined within it are met to capture a record of the changes made by an internal user.</description>
    <environments>Default</environments>
    <formulas>
        <name>ffSessionObject</name>
        <dataType>String</dataType>
        <expression>&quot;Asset | &quot; &amp; {!$Record.Name}</expression>
    </formulas>
    <formulas>
        <description>The match ID for an asset. Uses the existing match ID if there is one, else uses the ID of the asset.</description>
        <name>matchId</name>
        <dataType>String</dataType>
        <expression>IF( ISBLANK( {!$Record.match_id__c} ), 
{!$Record.Id}, 
{!$Record.match_id__c} )</expression>
    </formulas>
    <formulas>
        <name>recordTypeConversion</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.RecordType.DeveloperName}, 
 &quot;Business_Ownership&quot;, &quot;0124J000000xyJjQAI&quot;,
&quot;Other_Asset&quot;, &quot;0124J000000xtfYQAQ&quot;,
&quot;Property&quot;, &quot;0124J000000xtfTQAQ&quot;, 
&quot;0124J000000xtfYQAQ&quot;
)</expression>
    </formulas>
