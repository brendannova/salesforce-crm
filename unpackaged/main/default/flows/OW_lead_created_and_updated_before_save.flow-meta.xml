    <decisions>
        <description>Checks if a UK mobile number has been entered in the phone field while the mobile is blank and maps it across if so.</description>
        <name>Mobile_in_phone_field</name>
        <label>Mobile in phone field?</label>
        <locationX>314</locationX>
        <locationY>431</locationY>
        <defaultConnector>
            <targetReference>Lead_type</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.MobilePhone</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Phone</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Phone</leftValueReference>
                <operator>StartsWith</operator>
                <rightValue>
                    <stringValue>+447</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Swap_number</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
