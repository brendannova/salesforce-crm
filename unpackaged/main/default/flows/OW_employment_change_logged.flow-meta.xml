    <description>This flow will run whenever an FF employment record is created. This will check whether any unvalidated FF employment records need overwriting due to the creation of this new update.</description>
    <environments>Default</environments>
    <formulas>
        <name>dontCaptureChange</name>
        <dataType>Number</dataType>
        <expression>{!$Record.Employment__r.OW_stop_internal_change_processing__c} + 1</expression>
        <scale>0</scale>
    </formulas>
    <interviewLabel>Employment | change created {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Employment | change logged (FF)</label>
