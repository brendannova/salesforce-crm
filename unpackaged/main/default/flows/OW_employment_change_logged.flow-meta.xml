    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <description>Checks for an existing employment record using the match ID in case it has been created but not yet specified on the FF records being submitted by a client.</description>
        <name>Check_for_employment_record</name>
        <label>Check for employment record</label>
        <locationX>534</locationX>
        <locationY>1247</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Employment_found</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>match_id__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Match_id__c</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>FinServ__Employment__c</object>
        <queriedFields>Id</queriedFields>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordLookups>
        <description>Checks for an unvalidated FF employment record for the same employment so that this can be overwritten with the latest changes. 

Only checks for 1 record as this flow runs on creation of every FF employment record so there should never be more than 1 unvalidated record at a time.</description>
        <name>Check_for_unvalidated_FF_employment</name>
        <label>Check for unvalidated FF employment</label>
        <locationX>182</locationX>
        <locationY>455</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Records_to_overwrite</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>NotEqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <filters>
            <field>Match_id__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Match_id__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Match_id__c</elementReference>
            </value>
        </filters>
        <filters>
            <field>Validated__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>FF_Employment__c</object>
        <queriedFields>Id</queriedFields>
        <queriedFields>Validated__c</queriedFields>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
