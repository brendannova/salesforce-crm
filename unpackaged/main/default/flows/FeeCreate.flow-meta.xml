    <formulas>
        <name>finalFee</name>
        <dataType>Number</dataType>
        <expression>IF( {!Loop_3_Fee_models.DefaultOverrideable__c}, 
    IF( ISPICKVAL({!Loop_3_Fee_models.Format__c}, &quot;Percentage&quot;), {!AmountOverrideablePercentage} - BLANKVALUE({!Discount},0), {!Amount_fixed}-BLANKVALUE({!Discount},0)), {!Loop_3_Fee_models.DefaultAmount__c} - BLANKVALUE({!Discount},0))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fixedFeeFormula</name>
        <dataType>Number</dataType>
        <expression>IF( ISPICKVAL({!Loop_3_Fee_models.Format__c}, &quot;Percentage&quot;), 0, IF({!Loop_3_Fee_models.DefaultOverrideable__c},{!Amount_fixed}-IF({!Loop_3_Fee_models.Discountable__c},{!Discount},0), {!Loop_3_Fee_models.DefaultAmount__c}-IF({!Loop_3_Fee_models.Discountable__c},{!Discount},0)))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>percentageFeeFormula</name>
        <dataType>Number</dataType>
        <expression>IF( ISPICKVAL({!Loop_3_Fee_models.Format__c}, &quot;Percentage&quot;), 
    IF({!Loop_3_Fee_models.DefaultOverrideable__c},
        {!AmountOverrideablePercentage}-IF({!Loop_3_Fee_models.Discountable__c},{!Discount},0), 
        {!Loop_3_Fee_models.DefaultAmount__c}-IF({!Loop_3_Fee_models.Discountable__c},{!Discount},0)),
     0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>percentageFormula</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!Loop_3_Fee_models.Format__c}, &quot;Percentage&quot;), &quot;%&quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>poundFormula</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!Loop_3_Fee_models.Format__c}, &quot;Percentage&quot;), &quot;&quot;, &quot;£&quot;)</expression>
    </formulas>
    <formulas>
        <name>smDefault</name>
        <dataType>Boolean</dataType>
        <expression>IF({!Loop_3_Fee_models.Name}=&quot;Ongoing advice fee&quot;, True, False)</expression>
    </formulas>
    <formulas>
        <name>standardModel</name>
        <dataType>Boolean</dataType>
        <expression>IF( {!Loop_3_Fee_models.Product__r.ProductType__r.IsProtection__c}, false, {!Standard_model})</expression>
    </formulas>
    <formulas>
        <name>takenfromProductFormula</name>
        <dataType>String</dataType>
        <expression>IF({!Taken_from}= &quot;An existing product&quot;, {!productToTakeFrom.firstSelectedRow.Id}, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>takenFromRecommendationFormula</name>
        <dataType>String</dataType>
        <expression>IF( {!Taken_from}=&quot;This product&quot; || {!Loop_3_Fee_models.Product__r.ProductType__r.IsProtection__c}, &quot;replace&quot;, {!recToTakefrom.firstSelectedRow.Id})</expression>
    </formulas>
