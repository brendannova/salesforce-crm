        <locationX>1766</locationX>
        <locationY>1490</locationY>
        <defaultConnector>
            <targetReference>Update_fulfilment_audit_log</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>YesSkipAudit</name>
            <conditionLogic>(1 OR 3 OR 4) AND 2</conditionLogic>
            <conditions>
                <leftValueReference>Get_fulfilment.FulfilmentType__r.RecommendationTypeReference__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>UPDATE_EOW</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_fulfilment.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Complete</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_fulfilment.FulfilmentType__r.RecommendationTypeReference__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>NON_STANDARD_ACTION</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_fulfilment.FulfilmentType__r.RecommendationTypeReference__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>CLIENT_ACTION</stringValue>
                </rightValue>
            </conditions>
            <label>Yes</label>
        </rules>
    </decisions>
