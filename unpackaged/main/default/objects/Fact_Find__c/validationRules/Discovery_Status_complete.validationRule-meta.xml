<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Discovery_Status_complete</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
CONTAINS(RecordType.Name, &quot;Discovery&quot;),
ISPICKVAL(FF_Status__c , &quot;Complete&quot;),
ISNULL( Est_Liquid_Assets__c ) 
)</errorConditionFormula>
    <errorMessage>You must complete the &quot;Risk questions completed&quot; and &quot;Est Liquid Assets&quot; fields</errorMessage>
</ValidationRule>
