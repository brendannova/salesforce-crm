<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Primary_Phone__c</fullName>
    <description>Pulls the phone number from the account assigned to the opportunity</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Account.FinServ__PrimaryContact__r.Phone),Account.FinServ__PrimaryContact__r.MobilePhone,Account.FinServ__PrimaryContact__r.Phone)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This is pulled from the account above</inlineHelpText>
    <label>Primary Phone</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
