<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ChecklistType__c</fullName>
    <description>Calculates where the advice checklist is held based on whether we have a Salesforce checklist, a link to a Process Street one, or nothing.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(AdviceChecklist__c)), &quot;SALESFORCE&quot;, IF(NOT(ISBLANK(Suitability_checklist__c)), &quot;PROCESS_STREET&quot;, &quot;NONE&quot;))</formula>
    <label>Checklist Type</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
