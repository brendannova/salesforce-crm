<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OW_review_fact_find_status__c</fullName>
    <description>Stores the status of the review fact find so that users are aware of whether the customer can see it as well as manually control this.</description>
    <inlineHelpText>Only applies to customers on the new profile. You can use this to manually control visibility of the fact find or to indicate that we have skipped it for this customer.</inlineHelpText>
    <label>Review fact find status</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <controllingField>OW_status__c</controllingField>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Not visible to customer</fullName>
                <default>true</default>
                <label>Not visible to customer</label>
            </value>
            <value>
                <fullName>Visible to customer</fullName>
                <default>false</default>
                <label>Visible to customer</label>
            </value>
            <value>
                <fullName>Complete</fullName>
                <default>false</default>
                <label>Complete</label>
            </value>
            <value>
                <fullName>Skipped</fullName>
                <default>false</default>
                <label>Skipped</label>
            </value>
        </valueSetDefinition>
        <valueSettings>
            <controllingFieldValue>Prepare</controllingFieldValue>
            <controllingFieldValue>Catch up</controllingFieldValue>
            <controllingFieldValue>Await fact find</controllingFieldValue>
            <controllingFieldValue>Closed</controllingFieldValue>
            <valueName>Not visible to customer</valueName>
        </valueSettings>
        <valueSettings>
            <controllingFieldValue>Prepare</controllingFieldValue>
            <controllingFieldValue>Catch up</controllingFieldValue>
            <controllingFieldValue>Await fact find</controllingFieldValue>
            <controllingFieldValue>Complete</controllingFieldValue>
            <controllingFieldValue>Provide advice</controllingFieldValue>
            <controllingFieldValue>Closed</controllingFieldValue>
            <valueName>Skipped</valueName>
        </valueSettings>
        <valueSettings>
            <controllingFieldValue>Catch up</controllingFieldValue>
            <controllingFieldValue>Await fact find</controllingFieldValue>
            <valueName>Visible to customer</valueName>
        </valueSettings>
        <valueSettings>
            <controllingFieldValue>Catch up</controllingFieldValue>
            <controllingFieldValue>Await fact find</controllingFieldValue>
            <controllingFieldValue>Complete</controllingFieldValue>
            <controllingFieldValue>Provide advice</controllingFieldValue>
            <controllingFieldValue>Closed</controllingFieldValue>
            <valueName>Complete</valueName>
        </valueSettings>
    </valueSet>
</CustomField>
