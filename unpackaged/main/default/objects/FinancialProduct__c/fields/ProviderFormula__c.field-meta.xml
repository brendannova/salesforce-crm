<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ProviderFormula__c</fullName>
    <description>Displays the provider of the financial product. If the public provider is blank, display custom provider text name instead.</description>
    <externalId>false</externalId>
    <formula>if(ISBLANK(PublicProviderList__c), CustomProviderName__c, PublicProviderList__r.Name)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Provider</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
