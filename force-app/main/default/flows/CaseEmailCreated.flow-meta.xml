    <decisions>
        <description>When case is created from email, AccountId is set to the contact record if found.</description>
        <name>Does_account_Id_need_updating</name>
        <label>Does account Id need updating?</label>
        <locationX>380</locationX>
        <locationY>323</locationY>
        <defaultConnector>
            <targetReference>Get_household</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No update needed</defaultConnectorLabel>
        <rules>
            <name>Account_Id_needs_updating</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Account.IsPersonAccount</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>get_ACR</targetReference>
            </connector>
            <label>Account Id needs updating</label>
        </rules>
    </decisions>
    <decisions>
        <name>Does_owner_need_updating</name>
        <label>Does owner need updating?</label>
        <locationX>380</locationX>
        <locationY>1031</locationY>
        <defaultConnector>
            <targetReference>Get_entitlements</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Leave in client email queue</defaultConnectorLabel>
        <rules>
            <name>Update_owner</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_household.OW_dedicated_specialist__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_household.OW_dedicated_specialist__r.OutOfOfficeMessage</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.CreatedSource__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Manual</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_owner_to_DS</targetReference>
            </connector>
            <label>Update owner to DS</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_related_account_found</name>
        <label>Is related account found?</label>
        <locationX>182</locationX>
        <locationY>539</locationY>
        <defaultConnector>
            <isGoTo>true</isGoTo>
            <targetReference>Does_owner_need_updating</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Related account not found</defaultConnectorLabel>
        <rules>
            <name>Related_account_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>get_ACR</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Account_Id</targetReference>
            </connector>
            <label>Related account found</label>
        </rules>
    </decisions>
