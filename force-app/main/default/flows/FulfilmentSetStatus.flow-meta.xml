            <conditions>
                <leftValueReference>Get_pre_requisite_fulfilment.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Awaiting pre-requisites</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_pre_requisite_fulfilment</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_status_to_Submit_fulfilment</targetReference>
            </connector>
            <label>Ready</label>
        </rules>
    </decisions>
