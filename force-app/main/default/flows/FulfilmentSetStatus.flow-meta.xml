    <decisions>
        <name>Are_any_forms_required</name>
        <label>Are any forms required?</label>
        <locationX>2145</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>Update_status_to_Awaiting_pre_requisites</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Yes</defaultConnectorLabel>
        <rules>
            <name>NoForms</name>
            <conditionLogic>(1 OR 2 OR 5) AND (3 OR 4)</conditionLogic>
            <conditions>
                <leftValueReference>fulfilment.ExpressionOfWishFormStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Signed and saved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fulfilment.FulfilmentType__r.ExpressionOfWishFormRequirement__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Not required</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fulfilment.TransferFormsStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Signed and saved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fulfilment.FulfilmentType__r.TransferFormsRequirement__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Not required</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fulfilment.ExpressionOfWishFormStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Bypassed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Do_we_need_additional_AML</targetReference>
            </connector>
            <label>No</label>
        </rules>
    </decisions>
    <decisions>
        <name>Do_we_have_bank_details</name>
        <label>Do we have bank details?</label>
        <locationX>2096</locationX>
        <locationY>458</locationY>
        <defaultConnector>
            <targetReference>Update_status_to_Awaiting_pre_requisitesBankdetails</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>YesBank</name>
            <conditionLogic>((1 AND (2 OR 3 OR 7)) OR 4) OR (5 OR 6 OR 8)</conditionLogic>
            <conditions>
                <leftValueReference>fulfilment.BankDetailStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Received</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fulfilment.EmployerBankAccountStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Not required</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fulfilment.EmployerBankAccountStatus__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Submitted</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fulfilment.Recommendation__r.PrimaryOwner__r.FinServ__Age__pc</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>18.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recommendation.RecommendationType__r.ReferenceName__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>CLIENT_ACTION</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>recommendation.RecommendationType__r.ReferenceName__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>UPDATE_EOW</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fulfilment.EmployerBankAccountStatus__c</leftValueReference>
                <operator>EqualTo</operator>
            </conditions>
            <conditions>
                <leftValueReference>recommendation.RecommendationType__r.ReferenceName__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>CANCEL_PROTECTION</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_there_a_pre_requisite_action</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
