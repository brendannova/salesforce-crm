        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <start>
        <locationX>2696</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>What_type_is_this_for</targetReference>
        </connector>
        <filterFormula>ISBLANK({!$Record.Fulfilment_Checklist__c}) &amp;&amp;
(
((ISCHANGED({!$Record.Status__c})||ISNEW()) &amp;&amp; ISPICKVAL({!$Record.Checklist_Type__c}, &quot;External transfer in&quot;))
||
(ISPICKVAL({!$Record.Status__c}, &quot;Submit forms&quot;) &amp;&amp; NOT(ISPICKVAL({!$Record.Checklist_Type__c}, &quot;External transfer in&quot;)))
)</filterFormula>
        <object>Fulfillment__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
