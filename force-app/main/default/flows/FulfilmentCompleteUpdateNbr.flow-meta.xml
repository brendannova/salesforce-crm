        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>Get_fees</collectionReference>
        <conditionLogic>formula_evaluates_to_true</conditionLogic>
        <connector>
            <targetReference>Initial_fees</targetReference>
        </connector>
        <formula>{!currentItem_Get_initial_fees_0.FeeModel__r.Name} = &quot;Transfer fee&quot;</formula>
    </collectionProcessors>
    <decisions>
        <name>Is_this_newly_completed</name>
        <label>Is this newly completed?</label>
        <locationX>182</locationX>
        <locationY>252</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.DateComplete__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>yesterday</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_fees</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>Scheduled flow to update new new business register once fulfilment is complete</description>
    <environments>Default</environments>
    <formulas>
        <name>estimatedInitial</name>
        <dataType>Number</dataType>
        <expression>{!NbrToUpdate.EstimatedRevenueFromInitialFees__c} + IF(ISPICKVAL({!Initial_fees.FeeModel__r.Format__c}, &quot;Percentage&quot;), {!Initial_fees.PercentageFee__c}*0.01*{!Initial_fees.AppliesToRecommendation__r.Amount__c}, {!Initial_fees.FixedFee__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>estimatedOngoing</name>
        <dataType>Number</dataType>
        <expression>{!NbrToUpdate.EstimatedRevenueFromOngoingFees__c} + {!Ongoing_fees.PercentageFee__c} * 0.01 * {!Ongoing_fees.Fulfilment__r.Recommendation__r.Amount__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>estimatedTransfer</name>
        <dataType>Number</dataType>
        <expression>{!Transfer_fees.PercentageFee__c} * 0.01 * {!Transfer_fees.Fulfilment__r.Recommendation__r.Amount__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>receivedInitial</name>
        <dataType>Number</dataType>
        <expression>{!NbrToUpdate.ReceivedRevenueFromInitialFees__c} + {!Initial_fees.AmountReceived__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>yesterday</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} - 1</expression>
    </formulas>
    <interviewLabel>Fulfilment | complete - update NBR {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Fulfilment | complete - update NBR</label>
    <loops>
