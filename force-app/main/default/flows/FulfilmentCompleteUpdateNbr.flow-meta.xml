    <collectionProcessors>
        <name>Get_initial_fees</name>
        <elementSubtype>FilterCollectionProcessor</elementSubtype>
        <label>Get initial fees</label>
        <locationX>50</locationX>
        <locationY>576</locationY>
        <assignNextValueToReference>currentItem_Get_initial_fees_0</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>Get_fees</collectionReference>
        <conditionLogic>formula_evaluates_to_true</conditionLogic>
        <connector>
            <targetReference>Get_ongoing_fees</targetReference>
        </connector>
        <formula>ISPICKVAL({!currentItem_Get_initial_fees_0.FeeModel__r.Duration__c}, &quot;One-off&quot;) &amp;&amp; {!currentItem_Get_initial_fees_0.FeeModel__r.RevenueDueTo__r.Id} = {!Get_NOVA_provider.Id}</formula>
    </collectionProcessors>
    <collectionProcessors>
