        <conditionLogic>formula_evaluates_to_true</conditionLogic>
        <connector>
            <targetReference>Get_transfer_fees</targetReference>
        </connector>
        <formula>ISPICKVAL({!currentItem_Get_initial_fees_0.FeeModel__r.Duration__c}, &quot;Ongoing&quot;) &amp;&amp; {!currentItem_Get_initial_fees_0.FeeModel__r.RevenueDueTo__r.Id} = {!Get_NOVA_provider.Id}</formula>
    </collectionProcessors>
    <collectionProcessors>
