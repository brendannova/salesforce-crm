    <environments>Default</environments>
    <formulas>
        <name>authorityLevelFormula</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Client&quot;), &quot;Client managed (None)&quot;,{!Authority_level})</expression>
    </formulas>
    <formulas>
        <name>authorityStatus</name>
        <dataType>String</dataType>
        <expression>IF( ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Client&quot;), &quot;Not required&quot;, IF( ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Third party provider&quot;), &quot;To be requested&quot;, &quot;Not required&quot;))</expression>
    </formulas>
    <formulas>
        <name>awaitingPreReqsFormula</name>
        <dataType>Boolean</dataType>
        <expression>IF(
ISBLANK({!ir.FinancialProductFinServ__r.FinServ__FinancialAccountNumber__c})
||
 {!ir.FinancialProductFinServ__r.Public_Provider_Name__r.Name} == &quot;Unknown&quot;
||
(
NOT(CONTAINS(TEXT({!ir.FinancialProductFinServ__r.FinServ__PrimaryOwner__r.FinServ__Status__c}),&quot;Junior&quot;))
||
NOT(ISBLANK({!ir.FinancialProductFinServ__r.RegisteredContact__c}))
), true, false)</expression>
    </formulas>
    <formulas>
        <name>builderStatus</name>
        <dataType>String</dataType>
        <expression>IF( ISPICKVAL({!typeSelect.firstSelectedRow.BuilderLevel__c}, &quot;Not required&quot;), &quot;Not required&quot;, &quot;Not started&quot;)</expression>
    </formulas>
    <formulas>
        <name>checklistStatus</name>
        <dataType>String</dataType>
        <expression>IF( ISPICKVAL({!typeSelect.firstSelectedRow.ChecklistLevel__c}, &quot;Not required&quot;), &quot;Not required&quot;, &quot;Not started&quot;)</expression>
    </formulas>
    <formulas>
        <name>infoRequestStatus</name>
        <dataType>String</dataType>
        <expression>IF( ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Client&quot;), &quot;To be sent&quot;, IF( ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Third party provider&quot;), &quot;To be sent&quot;, &quot;Not required&quot;))</expression>
    </formulas>
    <formulas>
        <name>statusFormula</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Client&quot;) &amp;&amp; ISBLANK({!advice.AllLoaInfoReceivedTimestamp__c}), &quot;AWAIT_IDG&quot;,IF({!awaitingPreReqsFormula},&quot;AWAIT_PRE_REQS&quot;,&quot;INFO_REQ_SEND&quot;))</expression>
    </formulas>
