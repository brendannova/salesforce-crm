        <rules>
            <name>YesLinkedAdvice</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>informationRequest.Advice__c</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
