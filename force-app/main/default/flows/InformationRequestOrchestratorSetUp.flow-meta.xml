    <formulas>
        <name>authorityLevelFormula</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Client&quot;), &quot;Client managed (None)&quot;,{!Authority_level})</expression>
    </formulas>
    <formulas>
        <name>authorityStatus</name>
        <dataType>String</dataType>
        <expression>IF( ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Third party provider&quot;), &quot;To be requested&quot;, &quot;Not required&quot;)</expression>
    </formulas>
    <formulas>
        <name>builderStatus</name>
        <dataType>String</dataType>
        <expression>IF( ISPICKVAL({!typeSelect.firstSelectedRow.BuilderLevel__c}, &quot;Not required&quot;), &quot;Not required&quot;, &quot;Not started&quot;)</expression>
    </formulas>
    <formulas>
        <name>checklistStatus</name>
        <dataType>String</dataType>
        <expression>IF( ISPICKVAL({!typeSelect.firstSelectedRow.ChecklistLevel__c}, &quot;Not required&quot;), &quot;Not required&quot;, &quot;Not started&quot;)</expression>
    </formulas>
    <formulas>
        <name>infoRequestStatus</name>
        <dataType>String</dataType>
        <expression>IF( ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Client&quot;) || ISPICKVAL({!typeSelect.firstSelectedRow.InformationSource__c}, &quot;Third party provider&quot;), &quot;To be sent&quot;, &quot;Not required&quot;)</expression>
    </formulas>
    <formulas>
        <name>isAwaitingPreReqs</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!informationRequest.FinancialProductFinServ__r.FinServ__FinancialAccountNumber__c})
||
 {!informationRequest.FinancialProductFinServ__r.Public_Provider_Name__r.DeveloperName__c} == &quot;UNKNOWN&quot;
||
(
{!isJuniorProduct}
&amp;&amp;
ISBLANK({!informationRequest.FinancialProductFinServ__r.RegisteredContact__c})
)</expression>
    </formulas>
    <formulas>
        <name>isJuniorProduct</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS({!informationRequest.FinancialProductFinServ__r.RecordType.DeveloperName}, &quot;Junior&quot;)
||
{!informationRequest.FinancialProductFinServ__r.FinServ__PrimaryOwner__r.FinServ__Age__pc} &lt; 18</expression>
    </formulas>
