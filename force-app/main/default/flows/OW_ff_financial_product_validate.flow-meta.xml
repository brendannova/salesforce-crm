    <formulas>
        <description>This value is used to avoid the creation of an FF record when updating the related FP but where the FP is specified on the FF record itself.</description>
        <name>dontCaptureChangeFinancialProductSpecified</name>
        <dataType>Number</dataType>
        <expression>{!ffFinancialProduct.Financial_Product__r.OW_stop_internal_change_processing__c} + 1</expression>
        <scale>0</scale>
