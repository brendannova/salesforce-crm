    <formulas>
        <description>If General Investment, ISA or protection it maps straight to the correct Pre-LOA Record Type</description>
        <name>RecordTypeMapping</name>
        <dataType>String</dataType>
        <expression>
/* Pensions */ 
IF({!ffFinancialProduct.RecordTypeId} = &quot;0124J000000xsyPQAQ&quot;,

CASE({!ffFinancialProduct.Display_Type__c},
&quot;Defined Benefit&quot;, &quot;0121t0000001eQDAAY&quot;,
&quot;Defined Contribution&quot;,&quot;0121t0000001jQFAAY&quot;,
&quot;Annuity&quot;,&quot;0121t0000001jQFAAY&quot;,
&quot;I don&#39;t know&quot;,&quot;0121t0000001jQFAAY&quot;,
&quot;0121t0000001jQFAAY&quot;)
, 
/* Cash Accounts */ 
IF({!ffFinancialProduct.RecordTypeId} = &quot;0124J000000xsyFQAQ&quot;, 
CASE({!ffFinancialProduct.Display_Type__c},
&quot;Current Account&quot;,&quot;0121t0000001eKxAAI&quot;,
