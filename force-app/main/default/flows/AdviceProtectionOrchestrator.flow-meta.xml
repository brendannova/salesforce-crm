    <environments>Default</environments>
    <formulas>
        <name>dataGatheringStatusFormula</name>
        <dataType>String</dataType>
        <expression>CASE({!InformationRequestsNeeded}, 
&quot;Yes&quot;, &quot;In progress&quot;,
&quot;No&quot;, &quot;Not required&quot;,
&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <description>The date on which the advice deck expires.</description>
        <name>deckExpiryDate</name>
        <dataType>Date</dataType>
        <expression>ADDMONTHS({!$Flow.CurrentDate},3)</expression>
    </formulas>
    <formulas>
        <name>profileUpdateStatusFormula</name>
        <dataType>String</dataType>
        <expression>CASE({!ProfileUpdatesNeeded}, 
&quot;Yes&quot;, &quot;In progress&quot;,
&quot;No&quot;, &quot;Not required&quot;,
