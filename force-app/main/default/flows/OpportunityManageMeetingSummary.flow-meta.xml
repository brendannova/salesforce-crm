    <description>Screen flow to create file processing request for meeting summary. If one already exists, display appropriate message.</description>
    <environments>Default</environments>
    <formulas>
        <name>meetingLinkValidation</name>
        <dataType>Boolean</dataType>
        <expression>BEGINS({!MeetingRecordingURL.value}, {!meetingLinkPrefix})
&amp;&amp;
NOT(CONTAINS(&quot;folders&quot;, {!MeetingRecordingURL.value}))</expression>
    </formulas>
    <formulas>
        <name>meetingLinkValidationRetry</name>
        <dataType>Boolean</dataType>
        <expression>BEGINS({!DiscoMeetingURL.value}, {!meetingLinkPrefix})
&amp;&amp;
NOT(CONTAINS(&quot;folders&quot;, {!DiscoMeetingURL.value}))</expression>
    </formulas>
    <formulas>
        <description>Strip the meeting recording URL and send google drive ID</description>
        <name>meetingRecordingFileId</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(LEFT({!meetingLinkUrl}, FIND(&quot;/view&quot;, {!meetingLinkUrl}) -1), {!meetingLinkPrefix}, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <description>Validate the meeting recording URL from user entry in the input screen</description>
        <name>MeetingUrlValidation</name>
        <dataType>Boolean</dataType>
        <expression>BEGINS({!meetingLinkUrl}, {!meetingLinkPrefix})
&amp;&amp;
NOT(CONTAINS(&quot;folders&quot;, {!meetingLinkUrl}))</expression>
    </formulas>
    <formulas>
        <description>Add a message to retry the file processing request once user is happy to proceed</description>
        <name>retryMessages</name>
        <dataType>String</dataType>
        <expression>CASE(
{!Get_File_Processing_Request.FailedCode__c},
&quot;RAISE_SALESFORCE_ISSUE&quot;, &quot;Whoops something went wrong. Please raise this as an issue in the #help-ai-video-summary channel&quot;,
&quot;NOT_FOUND&quot;, &quot;The link provided was not found in Google Drive, or is invalid. Please add a valid link below and try again.&quot;,
&quot;SERVICE_UNAVAILABLE&quot;, &quot;Our AI server is down. The Product team has been made aware. We&#39;ll let you know when to try again.&quot;,
&quot;TOO_MANY_REQUESTS&quot;, &quot;Our AI service is busy with other summary requests right now. Please try back in 20 minutes.&quot;,
&quot;INTERNAL_SERVER_ERROR&quot;, &quot;Whoops something went wrong. Please raise this as an issue in the recent release help channel&quot;,
&quot;Failed code not found. Please raise a ticket with the product team&quot;)</expression>
    </formulas>
