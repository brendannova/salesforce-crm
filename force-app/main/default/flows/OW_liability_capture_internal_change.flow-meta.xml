        <dataType>String</dataType>
        <expression>IF(ISBLANK({!$Record.match_id__c}), 
{!$Record.Id}, 
{!$Record.match_id__c}
)</expression>
    </formulas>
    <formulas>
        <name>recordTypeIdMapping</name>
        <dataType>String</dataType>
