    <description>Runs whenever an event booked through Calendly should be created in Salesforce.</description>
    <environments>Default</environments>
    <formulas>
        <name>calendlyEventError</name>
        <dataType>String</dataType>
        <expression>&quot;Calendly event error for: &quot; &amp; {!NewCalendlyAction.Id}</expression>
    </formulas>
    <formulas>
        <description>Takes the email address in novawm.com format and creates an octopuswealth.com version of it. This allows us to support Calendly events where publisher email is either on the new Nova domain or old Octopus Wealth domain.</description>
        <name>emailOctopusWealthFormat</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!NewCalendlyAction.EventPrimaryPublisherEmail__c}, &quot;@octopuswealth.com&quot;, &quot;@novawm.com&quot;)</expression>
    </formulas>
    <formulas>
        <name>RecordTypeMapping</name>
        <dataType>String</dataType>
        <expression>IF(
	CONTAINS({!NewCalendlyAction.EventTypeSlug__c}, &quot;discovery-meeting&quot;), 
	&quot;0121t0000010QDhAAM&quot;, 
	IF(
		CONTAINS({!NewCalendlyAction.EventTypeSlug__c},&quot;annual-review-meeting&quot;), 
		&quot;0124J000000sEw3QAE&quot;,
		IF(
			CONTAINS({!NewCalendlyAction.EventTypeSlug__c},&quot;risk-call&quot;), 
			&quot;0124J000000d0lKQAQ&quot;, 
			IF(
			CONTAINS({!NewCalendlyAction.EventTypeSlug__c},&quot;advice-meeting&quot;), 
			&quot;0121t0000005w5oAAA&quot;, 
			&quot;0121t0000001jcpAAA&quot;
			)
		)
	)
)</expression>
    </formulas>
