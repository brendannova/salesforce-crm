    <formulas>
        <name>confirmModalMessage</name>
        <dataType>String</dataType>
        <expression>IF({!DataGatheringNotRequiredNow.renderNext}, &quot;Are you sure you want to mark data gathering as not required? Any information requests will delinked and the advice will complete data gathering without them.&quot;,
IF({!DataGatheringRequiredNow.renderNext}, &quot;Are you sure you want to mark data gathering as required? This advice will complete data gathering when associated information requests receive their information.&quot;,
IF({!DataGatheringProceedWhileInProgress.renderNext}, &quot;Associated information requests are not yet at information received. Are you sure you want to mark them as complete?&quot;,
&quot;&quot;)))</expression>
    </formulas>
    <formulas>
        <description>Contextual help for the data gathering status field.</description>
        <name>informationRequestsHelp</name>
        <dataType>String</dataType>
        <expression>CASE({!advice.DataGatheringStatus__c},
&quot;Not required&quot;, &quot;No information requests are needed for this advice.&quot;,
&quot;In progress&quot;, &quot;This will update to complete automatically when all associated information requests have their information.&quot;,
&quot;Complete&quot;, &quot;&quot;,
&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>postReviewFactFindNotesCompleteDisabled</name>
        <dataType>Boolean</dataType>
        <expression>ISBLANK({!advice.OW_specialist_notes__c}) || 
ISBLANK({!advice.Additional_Questions_to_be_asked_on_call__c})</expression>
    </formulas>
    <formulas>
        <name>postReviewFactFindNotesCompleteHelp</name>
        <dataType>String</dataType>
        <expression>IF(NOT({!postReviewFactFindNotesCompleteDisabled}), &quot;Complete task when ready&quot;,
&quot;Please complete: &quot; &amp; 
IF(ISBLANK({!advice.OW_specialist_notes__c}),  &quot;Specialist notes;  &quot;, &quot;&quot;) &amp;
IF(ISBLANK({!advice.Additional_Questions_to_be_asked_on_call__c}), &quot;Outstanding information; &quot;, &quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>profileUpdateStatusHelp</name>
        <dataType>String</dataType>
        <expression>IF({!adviceType} = &quot;ANNUAL_REVIEW_ACCUMULATION&quot; || {!adviceType} = &quot;ANNUAL_REVIEW_RETIREMENT&quot;,
&quot;Moves to complete automatically when the review fact find is completed or skipped&quot;,
&quot;Update as appropriate to track progress&quot;)</expression>
    </formulas>
    <formulas>
        <name>profileUpdateStatusPicklistDisabled</name>
