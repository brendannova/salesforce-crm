<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>55.0</apiVersion>
    <description>Called by the Account | Party state manager flow, this sub-flow coordinates all actions for a prospect -&gt; verifying transition for a person.</description>
    <environments>Default</environments>
    <interviewLabel>Account | Prospect to verifying actions {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Account | Prospect to verifying actions</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Kick_off_AML_process</targetReference>
        </connector>
    </start>
    <status>Active</status>
    <subflows>
        <description>Call the AML create sub-flow to kick off the AML process for this party, now that they are in the verifying status.</description>
        <name>Kick_off_AML_process</name>
        <label>Kick off AML process</label>
        <locationX>176</locationX>
        <locationY>134</locationY>
        <connector>
            <targetReference>Kick_off_information_request_process</targetReference>
        </connector>
        <flowName>OW_aml_create</flowName>
        <inputAssignments>
            <name>party</name>
            <value>
                <elementReference>party</elementReference>
            </value>
        </inputAssignments>
    </subflows>
    <subflows>
        <name>Kick_off_information_request_process</name>
        <label>Kick off information request process</label>
        <locationX>176</locationX>
        <locationY>242</locationY>
        <flowName>TobAccountInformationRequestCreateInformationRequests</flowName>
        <inputAssignments>
            <name>client</name>
            <value>
                <elementReference>party</elementReference>
            </value>
        </inputAssignments>
    </subflows>
    <variables>
        <name>NewAMLCheck</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>AML__c</objectType>
    </variables>
    <variables>
        <description>Stores the details of the party who has gone from prospect to verifying.</description>
        <name>party</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
        <objectType>Account</objectType>
    </variables>
</Flow>
