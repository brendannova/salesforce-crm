    <fullName>EmailName__c</fullName>
    <description>Hyperlink to email message</description>
    <externalId>false</externalId>
    <formula>HYPERLINK( 

 &quot;/lightning/r/EmailMessage/&quot; &amp; Id &amp;&quot;/view&quot;, 

