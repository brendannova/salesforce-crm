<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EssentialSpendingMonthly__c</fullName>
    <description>Takes the essential spending at any frequency and converts to monthly</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL(OW_essential_spending_frequency__c, &quot;Weekly&quot;),  52*OW_essential_spending__c/12, IF(ISPICKVAL(OW_essential_spending_frequency__c, &quot;Monthly&quot;), OW_essential_spending__c,
IF(ISPICKVAL(OW_essential_spending_frequency__c, &quot;Annually&quot;), OW_essential_spending__c/12,0)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Essential Spending (Monthly)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
