<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ChildcareSpendingMonthly__c</fullName>
    <description>Takes spending of childcare at any frequency and returns monthly spend</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL( Cost_of_Childcare_Frequency__c , &quot;Weekly&quot;),52*Cost_of_Childcare__c/12, IF(ISPICKVAL(Cost_of_Childcare_Frequency__c,&quot;Monthly&quot;),Cost_of_Childcare__c,
IF(ISPICKVAL(Cost_of_Childcare_Frequency__c,&quot;Annually&quot;), Cost_of_Childcare__c/12,0)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Childcare Spending (Monthly)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
