<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_is_Mandatory_if_displayed_on_TL</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
TEXT( DisplayOnLifeline__c) = &#39;Yes&#39;,
ISBLANK( Date__c ))</errorConditionFormula>
    <errorMessage>Must provide a date if Display on Timeline is Yes</errorMessage>
</ValidationRule>
