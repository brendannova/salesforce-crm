<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CannotUpdateCashAccountTypes</fullName>
    <active>true</active>
    <description>Prevent updating of cash account financial products since these have moved to CheerIO.</description>
    <errorConditionFormula>((RecordType.DeveloperName = &#39;SavingsAccount&#39;
||
RecordType.DeveloperName = &#39;FP_Current_Account&#39;
||
RecordType.DeveloperName = &#39;Premium_Bond&#39;
||
RecordType.DeveloperName = &#39;Offset_Mortgage_Cash_Account&#39;
||
RecordType.DeveloperName = &#39;FP_Fixed_Term_Deposit&#39;))
&amp;&amp;
NOT( $Permission.ValidationBypass )</errorConditionFormula>
    <errorMessage>You can no longer update cash accounts from here. Please use the migrated CheerIO version of this.</errorMessage>
</ValidationRule>
