<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IO_ID_Numbers_Only</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
NOT(ISNUMBER(IO_Plan_ID__c)),
NOT(ISBLANK(IO_Plan_ID__c))
)</errorConditionFormula>
    <errorMessage>IO Plan ID must contain only numbers</errorMessage>
</ValidationRule>
