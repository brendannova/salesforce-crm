<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BusinessType__c</fullName>
    <description>This field captures whether a financial product is pre-existing or new business and is used to control visibility and valuation source across the profile and dashboard.</description>
    <inlineHelpText>Only choose new business if this financial product was recommended by Nova.</inlineHelpText>
    <label>Business Type</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>New business</fullName>
                <default>false</default>
                <label>New business</label>
            </value>
            <value>
                <fullName>Pre-existing</fullName>
                <default>false</default>
                <label>Pre-existing</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
