<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CompletedBy__c</fullName>
    <description>Captures who the RTQ was completed by. Defaults to the customer but in rare exceptions, an RTQ will be completed by a colleague and we want to capture that this was the case.</description>
    <label>Completed by</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Customer</fullName>
                <default>true</default>
                <label>Customer</label>
            </value>
            <value>
                <fullName>Colleague</fullName>
                <default>false</default>
                <label>Colleague</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
