<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MyDueOverdueTasks</fullName>
    <booleanFilter>(1 OR 2) AND 3</booleanFilter>
    <columns>DUE_DATE</columns>
    <columns>SUBJECT</columns>
    <columns>WHO_NAME</columns>
    <columns>WHAT_NAME</columns>
    <columns>FinServ__Household__c</columns>
    <columns>PRIORITY</columns>
    <columns>STATUS</columns>
    <columns>CREATED_DATE</columns>
    <filterScope>Mine</filterScope>
    <filters>
        <field>OW_target_start_date__c</field>
        <operation>lessOrEqual</operation>
        <value>TODAY</value>
    </filters>
    <filters>
        <field>DUE_DATE</field>
        <operation>lessOrEqual</operation>
        <value>TODAY</value>
    </filters>
    <filters>
        <field>STATUS</field>
        <operation>equals</operation>
        <value>Open,Not Started,In Progress,On Hold</value>
    </filters>
    <label>My Due/Overdue Tasks</label>
</ListView>
