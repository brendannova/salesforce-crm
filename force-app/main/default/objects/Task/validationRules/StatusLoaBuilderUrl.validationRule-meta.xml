<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StatusLoaBuilderUrl</fullName>
    <active>true</active>
    <description>Cannot be set to complete if relatedLOA Builder tab url is blank</description>
    <errorConditionFormula>!$Setup.SystemConfiguration__c.ValidationBypass__c &amp;&amp;
AND(
 OW_task_information_identifier__c = &quot;send_loa_to_provider&quot;,
ISPICKVAL( Status, &quot;Completed&quot;),
ISBLANK(Related_LOA__r.LOA_Builder_Tab_URL__c)
)</errorConditionFormula>
    <errorMessage>You need to add the LOA builder&#39;s URL link before completing this task</errorMessage>
</ValidationRule>
