<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Chase_Full_Fact_Find</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  RecordType.DeveloperName  = &apos;Chase_Full_Fact_Find&apos;,
 ISPICKVAL(Status, &quot;Completed&quot;), 
 NOT(ISPICKVAL(Related_Fact_Find__r.FF_Status__c , &quot;Complete&quot;))
)</errorConditionFormula>
    <errorDisplayField>Status</errorDisplayField>
    <errorMessage>You need to mark the Fact Find as complete.</errorMessage>
</ValidationRule>
