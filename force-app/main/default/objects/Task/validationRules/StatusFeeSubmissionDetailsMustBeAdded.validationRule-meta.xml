<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StatusFeeSubmissionDetailsMustBeAdded</fullName>
    <active>true</active>
    <description>Details about fee submission must be completed before submit fees tasks can be completed</description>
    <errorConditionFormula>!$Setup.SystemConfiguration__c.ValidationBypass__c &amp;&amp;
(OW_task_information_name__c = &quot;submit_ongoing_fee&quot; ||
OW_task_information_name__c = &quot;submit_initial_fee&quot;)
&amp;&amp;
ISBLANK(RelatedFee__r.DateOfSubmission__c )
&amp;&amp;
NOT(ISBLANK(RelatedFee__c))
&amp;&amp;
ISPICKVAL(Status, &quot;Completed&quot;)</errorConditionFormula>
    <errorMessage>Please complete the fee details</errorMessage>
</ValidationRule>
