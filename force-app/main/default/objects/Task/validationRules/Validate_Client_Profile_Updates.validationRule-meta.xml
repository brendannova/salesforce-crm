<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Client_Profile_Updates</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 RecordType.DeveloperName  = &quot;Validate_Client_Profile_Updates&quot;,
 ISPICKVAL(Status, &quot;Completed&quot;),
FinServ__Household__r.Fact_Find_Validations_Outstanding__c = true

)</errorConditionFormula>
    <errorDisplayField>Status</errorDisplayField>
    <errorMessage>There are still Profile Validations outstanding</errorMessage>
</ValidationRule>
