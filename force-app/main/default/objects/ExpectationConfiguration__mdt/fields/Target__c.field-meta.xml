    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Destination</fullName>
                <default>true</default>
                <label>Destination</label>
            </value>
            <value>
                <fullName>Source</fullName>
                <default>false</default>
                <label>Source</label>
            </value>
            <value>
                <fullName>Both</fullName>
                <default>false</default>
                <label>Both</label>
            </value>
        </valueSetDefinition>
