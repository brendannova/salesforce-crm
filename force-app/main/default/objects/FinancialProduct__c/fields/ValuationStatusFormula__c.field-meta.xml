<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValuationStatusFormula__c</fullName>
    <description>Formula to display a green tick when valuation of account is in date and a warning flag if valuation is out of date.</description>
    <externalId>false</externalId>
    <formula>IF(ValuationDate__c &gt;= ADDMONTHS(TODAY(), -3), IMAGE(&quot;/resource/ValueTick&quot;, &quot;Green&quot;, 25, 25),
IMAGE(&quot;/resource/ValueRefresh&quot;, &quot;Amber&quot;, 25, 25))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Valuation status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
