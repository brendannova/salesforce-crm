<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_to_conversion__c</fullName>
    <description>Number of weeks from introduction to conversion</description>
    <externalId>false</externalId>
    <formula>CEILING(
(DATETIMEVALUE(  ConvertedDate  )  - DATETIMEVALUE(DATEVALUE(CreatedDate)))/7
)</formula>
    <label>Weeks to conversion</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
