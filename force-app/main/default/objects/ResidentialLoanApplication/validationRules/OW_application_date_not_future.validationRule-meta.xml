<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OW_application_date_not_future</fullName>
    <active>true</active>
    <description>Ensures that the date the mortgage application submitted is not in the future.</description>
    <errorConditionFormula>OW_date_mortgage_application_submitted__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>OW_date_mortgage_application_submitted__c</errorDisplayField>
    <errorMessage>Date cannot be in the future</errorMessage>
</ValidationRule>
