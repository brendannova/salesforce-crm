<?xml version="1.0" encoding="UTF-8"?>
<ProfilePasswordPolicy xmlns="http://soap.sforce.com/2006/04/metadata">
    <forgotPasswordRedirect>false</forgotPasswordRedirect>
    <lockoutInterval>0</lockoutInterval>
    <maxLoginAttempts>5</maxLoginAttempts>
    <minimumPasswordLength>10</minimumPasswordLength>
    <minimumPasswordLifetime>true</minimumPasswordLifetime>
    <obscure>true</obscure>
    <passwordComplexity>2</passwordComplexity>
    <passwordExpiration>90</passwordExpiration>
    <passwordHistory>24</passwordHistory>
    <passwordQuestion>1</passwordQuestion>
    <profile>secure - external compliance</profile>
</ProfilePasswordPolicy>
